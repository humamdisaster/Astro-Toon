          <packagedElement xmi:type="uml:Class" xmi:id="_cB3I4LXfEfCwwoklm5k2fQ" name="GestorRondas">
            <ownedComment xmi:id="_iNC48LYEEfCwwoklm5k2fQ">
              <body>Clase responsable de gestionar las rondas del juego.&#xD;
Controla las condiciones de victoria y derrota, &#xD;
y gestiona las transiciones correspondientes entre pantallas.</body>
            </ownedComment>
            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_cVFm4LXfEfCwwoklm5k2fQ" name="manejarRondas" specification="_cOaXILXfEfCwwoklm5k2fQ">
              <ownedParameter xmi:id="_cVFm4bXfEfCwwoklm5k2fQ" name="juego" type="_cFazMLXfEfCwwoklm5k2fQ"/>
              <ownedParameter xmi:id="_cVFm4rXfEfCwwoklm5k2fQ" name="nave" type="_cDReILXfEfCwwoklm5k2fQ"/>
              <ownedParameter xmi:id="_cVFm47XfEfCwwoklm5k2fQ" name="enemigosCreados">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <ownedParameter xmi:id="_cVFm5LXfEfCwwoklm5k2fQ" name="enemigosMaxNivel">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <language>java</language>
              <body>// Condición de victoria&#xD;
	    if (juego.getEnemigos().isEmpty() &amp;&amp; enemigosCreados >= enemigosMaxNivel) {&#xD;
	        juego.setRondaCompletada(true); // activa bandera para transición&#xD;
	        return;&#xD;
	    }&#xD;
&#xD;
	    // Condición de derrota&#xD;
	    if (nave.estaDestruido()) {&#xD;
	        if (juego.getScore() > juego.getGame().getHighScore())&#xD;
	            juego.getGame().setHighScore(juego.getScore());&#xD;
&#xD;
	        Screen gameOver = new PantallaGameOver(juego.getGame());&#xD;
	        gameOver.resize(1200, 800);&#xD;
	        juego.getGame().setScreen(gameOver);&#xD;
	        juego.dispose();&#xD;
	    }&#xD;
	</body>
            </ownedBehavior>
