              <language>java</language>
              <body>super(tx, x, y, 3); 
     this.txBala = txBala;
     this.sonidoHerido = Gdx.audio.newSound(Gdx.files.internal(&quot;hit.mp3&quot;));
     this.soundBala = Gdx.audio.newSound(Gdx.files.internal(&quot;shoot.mp3&quot;));
