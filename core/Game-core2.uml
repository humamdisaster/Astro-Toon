            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_cVaXALXfEfCwwoklm5k2fQ" name="update" specification="_cO1N4LXfEfCwwoklm5k2fQ">
              <ownedParameter xmi:id="_cVaXAbXfEfCwwoklm5k2fQ" name="juego" type="_cFazMLXfEfCwwoklm5k2fQ"/>
              <language>java</language>
              <body>actualizarEstadoHerido();

     if (objetivo == null || objetivo.estaDestruido()) return;
     
     // Movimiento hacia la nave
     float dx = objetivo.getX() - spr.getX();
     float dy = objetivo.getY() - spr.getY();
     float distancia = (float)Math.sqrt(dx*dx + dy*dy);

     float velocidad = 4; // ajusta para que sea más rápida o lenta
     if (distancia > 0) {
         xVel = velocidad * dx / distancia;
         yVel = velocidad * dy / distancia;
     }

     mover();
 </body>
            </ownedBehavior>
